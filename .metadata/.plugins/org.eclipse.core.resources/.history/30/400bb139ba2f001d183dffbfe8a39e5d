#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int kervin[6];
	kervin=5;
	kervin=0;
	kervin=4;
	kervin=3;
	kervin=3;
	kervin=1;

	printf("mostrar el arrays de kervin indice dos %d", kervin[2]);

	return EXIT_SUCCESS;
}
