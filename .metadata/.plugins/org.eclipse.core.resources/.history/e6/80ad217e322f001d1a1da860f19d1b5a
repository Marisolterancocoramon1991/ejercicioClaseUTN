#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int kervin=30;
	int* pKervin;

	printf("la edad de kervin es %d\n", kervin);
	printf("la direccion de la celda que contiene kervin %p\n", &kervin);
	printf("la direccion de la celda que contiene punetor de kervin %p\n", &pKervin);


	return EXIT_SUCCESS;
}
