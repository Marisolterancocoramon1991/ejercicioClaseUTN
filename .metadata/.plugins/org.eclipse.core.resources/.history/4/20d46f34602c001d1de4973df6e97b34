#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int numeroUno;
	int numeroDos;
	int suma;

	printf("ingresar numero uno/n");
	fflush(stdin);
	scanf("%d", &numeroUno);


	printf("ingresar numero dos/n");
	fflush(stdin);
	scanf("%d", &numeroDos);

	suma=numeroUno+numeroDos;

	printf("el resultado es: %d", suma);
}
